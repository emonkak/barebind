{
  "name": "@emonkak/tempura",
  "version": "0.0.0",
  "main": "index.js",
  "type": "module",
  "author": "Shota Nozaki",
  "license": "MIT",
  "scripts": {
    "build": "wireit",
    "clean": "tsc --build --clean",
    "test": "node --no-warnings=ExperimentalWarning --loader ts-node/esm --test test/**/*.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/node": "^20.11.16",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "wireit": "^0.14.4"
  },
  "wireit": {
    "build": {
      "command": "tsc --build",
      "files": [
        "src/**/*.ts"
      ],
      "output": [
        "dist/**/*.js"
      ]
    }
  }
}
