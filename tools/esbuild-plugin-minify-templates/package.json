{
  "name": "esbuild-plugin-minify-templates",
  "version": "0.0.0",
  "exports": {
    ".": {
      "import": "./src/index.ts"
    }
  },
  "publishConfig": {
    "exports": {
      ".": {
        "import": "./lib/index.js",
        "types": "./lib/index.d.ts"
      }
    }
  },
